{
  "dashboard": {
    "id": null,
    "uid": "hybrid-search-dashboard",
    "title": "Hybrid Search Application Metrics",
    "tags": ["hybrid-search", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Duration (95th Percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ]
      },
      {
        "id": 3,
        "title": "Active Sessions",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "active_sessions",
            "refId": "A"
          }
        ],
        "prefix": "",
        "postfix": " sessions"
      },
      {
        "id": 4,
        "title": "System CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "cpu_percent",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage"
          }
        ]
      },
      {
        "id": 5,
        "title": "System Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "memory_percent",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage"
          }
        ]
      },
      {
        "id": 6,
        "title": "Search Requests by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(search_requests_total[5m])",
            "legendFormat": "{{search_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Searches/sec"
          }
        ]
      },
      {
        "id": 7,
        "title": "Search Duration by Type (95th Percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(search_duration_seconds_bucket[5m]))",
            "legendFormat": "{{search_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ]
      },
      {
        "id": 8,
        "title": "LLM Call Duration (95th Percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ]
      },
      {
        "id": 9,
        "title": "LLM Token Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "increase(llm_token_usage_input_total[5m])",
            "legendFormat": "{{model}} - Input Tokens",
            "refId": "A"
          },
          {
            "expr": "increase(llm_token_usage_output_total[5m])",
            "legendFormat": "{{model}} - Output Tokens",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens"
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "5s"
  }
}